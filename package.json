{
  "name": "halchemy",
  "version": "1.0.0",
  "description": "A chatbot space game",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "npm run test:run",
    "test:run": "mocha --recursive",
    "test:unit": "npm run test:run -- ./test/unit",
    "test:e2e": "npm run test:run -- ./test/e2e",
    "test:watch": "nodemon --exec 'npm run -s test:run'",
    "test:unit:watch": "nodemon --exec 'npm run -s test:unit'",
    "test:e2e:watch": "nodemon --exec 'npm run -s test:e2e'",
    "db-load-response-heroku": "mongoimport -h ds023418.mlab.com:23418 -d heroku_pghxnmz8 -c responses -u heroku_pghxnmz8 -p 8rsomit7fcl091fttcv678e7e3 --file ./test/db-scripts/responses.json",
    "db-load-responses": "mongoimport -h localhost:27017 -d halchemy -c responses  --drop --file ./test/db-scripts/responses.json",
    "db-load-stage-heroku": "mongoimport -h ds023418.mlab.com:23418 -d heroku_pghxnmz8 -c stages -u heroku_pghxnmz8 -p 8rsomit7fcl091fttcv678e7e3 --file ./test/db-scripts/stages.json",
    "db-load-stages": "mongoimport -h localhost:27017 -d halchemy -c stages  --drop --file ./test/db-scripts/stages.json",
    "db-load": "npm run db-load-responses && npm run db-load-stages",
    "db-load-heroku": "npm run db-load-response-heroku && npm run db-load-stage-heroku",
    "start": "node server.js",
    "start:watch": "nodemon server.js",
    "play": "node cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-dart/HALchemy.git"
  },
  "author": "Team Dart",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^5.1.0",
    "inquirer": "^6.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.4",
    "morgan": "^1.9.0",
    "@octokit/rest": "^14.0.5",
    "chalk": "^2.3.0",
    "clear": "0.0.1",
    "clui": "^0.3.6",
    "configstore": "^3.1.1",
    "figlet": "^1.2.0",
    "inquirer": "^5.0.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "simple-git": "^1.89.0",
    "touch": "^3.1.0"
  },
  "bugs": {
    "url": "https://github.com/team-dart/HALchemy/issues"
  },
  "homepage": "https://github.com/team-dart/HALchemy#readme"
}
